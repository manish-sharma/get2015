    <validationRules>
        <fullName>Billing_Zip_format</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
OR(BillingCountry = &quot;USA&quot;, BillingCountry = &quot;US&quot;),
NOT(REGEX(BillingPostalCode, &quot;\\d{5}(-\\d{4})?&quot;))
)</errorConditionFormula>
        <errorDisplayField>BillingPostalCode</errorDisplayField>
        <errorMessage>Billing Zip/Postal Code is in 99999 or 99999-9999 format</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Credit_Card_Number_validation</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(REGEX(Credit_Card_Number__c,&quot;(((\\d{4}-) {3}\\d{4})|\\d{16})?&quot;))</errorConditionFormula>
        <errorDisplayField>Credit_Card_Number__c</errorDisplayField>
        <errorMessage>Credit_Card_Number is formatted in 9999-9999-9999-9999 or 9999999999999999.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mct_pt</fullName>
        <active>true</active>
        <errorConditionFormula>Mix_Pct__c &lt; 0 || Mix_Pct__c &gt; 1</errorConditionFormula>
        <errorDisplayField>Mix_Pct__c</errorDisplayField>
        <errorMessage>Mix Pct is between 0 and 100%.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Number_should_be_in_numeric</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISBLANK( AccountNumber )), NOT((ISNUMBER(AccountNumber))))</errorConditionFormula>
        <errorDisplayField>AccountNumber</errorDisplayField>
        <errorMessage>Account Number should be numeric</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>multiple_of_five_validation</fullName>
        <active>true</active>
        <errorConditionFormula>MOD(multiple_of_five__c,5)  &lt;&gt; 0</errorConditionFormula>
        <errorDisplayField>multiple_of_five__c</errorDisplayField>
        <errorMessage>number should be multiple of 5</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>weekday_validation</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(CASE( MOD(  weekday__c  - DATE(1900, 1, 7), 7),
0, 1,
6, 1,
0) = 0)</errorConditionFormula>
        <errorDisplayField>weekday__c</errorDisplayField>
        <errorMessage>date must be weekday (not Saturday or Sunday)</errorMessage>
    </validationRules>
