<apex:page renderAs="pdf" standardController="Student__c" extensions="AttachPdf" action="{!savePdf}">
    <!-- Student Details-->
    <h1>Student Information</h1>
    <apex:form >
        <apex:pageBlock >
            <!-- To display Student Details -->
            <apex:pageBlockSection >
                First Name: {!Student__c.First_Name__c  }<br/>
                Last Name:{!Student__c.LastName__c  }<br/>
                Date of Birth: {!Student__c.Dob__c  }<br/>
                Age: {!Student__c.age__c  }<br/>
                Email: {!Student__c.email__c  }<br/>
                Class:{!Student__c.class__c  }<br/>
                Married: {!Student__c.Married__c  }<br/>
                Spouse NAme:{!Student__c.Wife_Name__c  }<br/>
                <apex:commandButton action="{! save }" value="Save" />
            </apex:pageBlockSection>
        </apex:pageBlock>
    </apex:form>
</apex:page>