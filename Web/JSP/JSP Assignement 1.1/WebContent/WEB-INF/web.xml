<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:web="http://xmlns.jcp.org/xml/ns/javaee">
  <display-name>RegistrationForm</display-name>
  <servlet>
    <display-name>myservlet</display-name>
    <servlet-name>RegistrationForm</servlet-name>
    <servlet-class>com.servelet.RegistrationForm</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegistrationForm</servlet-name>
    <url-pattern>/RegistrationForm</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>