<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from TemperatureConvertorService WSDL service -->

  <service name="TemperatureConvertor" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://com"/>
      <parameter name="wsdlServiceElement" value="TemperatureConvertorService"/>
      <parameter name="schemaQualified" value="http://com"/>
      <parameter name="wsdlServicePort" value="TemperatureConvertor"/>
      <parameter name="className" value="com.TemperatureConvertorSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="TemperatureConvertor"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="fahrenheitToCelsius" qname="operNS:fahrenheitToCelsius" xmlns:operNS="http://com" returnQName="retNS:fahrenheitToCelsiusReturn" xmlns:retNS="http://com" returnType="rtns:float" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:value" xmlns:pns="http://com" type="tns:float" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="celsiusTofahrenheit" qname="operNS:CelsiusTofahrenheit" xmlns:operNS="http://com" returnQName="retNS:CelsiusTofahrenheitReturn" xmlns:retNS="http://com" returnType="rtns:float" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:value" xmlns:pns="http://com" type="tns:float" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="celsiusTofahrenheit fahrenheitToCelsius"/>

  </service>
</deployment>
